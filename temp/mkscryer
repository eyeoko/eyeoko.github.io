#!/bin/bash
pushd ~/github.com/mthom/scryer-prolog > /dev/null
git pull
cargo build --release
#cargo test --release
popd > /dev/null
echo ""

pushd ~/github.com/eyereasoner/euler/cases > /dev/null
./test scryer-prolog
popd > /dev/null
echo ""

pushd ~/github.com/eyereasoner/eye2-scryer/etc > /dev/null
./test
popd > /dev/null
