PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX : <http://example.org/socrates#>
PREFIX r: <http://www.w3.org/2000/10/swap/reason#>

[] a r:Proof, r:Conjunction;
  r:component <#lemma1>;
  r:component <#lemma2>;
  r:gives {
    :Socrates a :Human.
    :Socrates a :Mortal.
  }.

<#lemma1> a r:Inference;
  r:gives {
    :Socrates a :Human.
  };
  r:evidence (
    <#lemma3>
  );
  r:rule <#lemma4>.

<#lemma2> a r:Inference;
  r:gives {
    :Socrates a :Mortal.
  };
  r:evidence (
    <#lemma5>
  );
  r:rule <#lemma4>.

<#lemma3> a r:Extraction;
  r:gives {
    :Socrates a :Human.
  };
  r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/josd/eye/looking/eye-wasm/examples/socrates.n3>].

<#lemma4> a r:Extraction;
  r:gives {
    {:Socrates a ?x_0_1} => {:Socrates a ?x_0_1}.
  };
  r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/josd/eye/looking/eye-wasm/examples/socrates-query.n3>].

<#lemma5> a r:Inference;
  r:gives {
    :Socrates a :Mortal.
  };
  r:evidence (
    <#lemma6>
    <#lemma3>
  );
  r:rule <#lemma7>.

<#lemma6> a r:Extraction;
  r:gives {
    :Human rdfs:subClassOf :Mortal.
  };
  r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/josd/eye/looking/eye-wasm/examples/socrates.n3>].

<#lemma7> a r:Extraction;
  r:gives {
    {?x_0_2 rdfs:subClassOf ?x_1_2.
     ?x_2_2 a ?x_0_2} => {?x_2_2 a ?x_1_2}.
  };
  r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/josd/eye/looking/eye-wasm/examples/socrates.n3>].

